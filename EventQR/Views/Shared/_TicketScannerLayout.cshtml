        <div class="container">
            <a class="navbar-brand" href="index.html"><i></i> BPST-Events</a>

            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false"
                    aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
                <span class="navbar-toggler-icon"></span>
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav mx-auto">
                    <li class="nav-item">
                        <a asp-action="GuestList" asp-controller="CheckIn" asp-area="Scanner" class="nav-link"><span data-hover="GuestList">Guest List</span></a>
                    </li>
                    <li class="nav-item">
                        <a asp-action="EventDetails" asp-controller="CheckIn" asp-area="Scanner" class="nav-link"><span data-hover="EventsDetails">Event Details</span></a>
                    </li>
                    <li class="nav-item">
                        <a href="/Account/LogOut" class="nav-link"><span data-hover="Log Out">Log Out</span></>
                    </li>
                 
                </ul>
                <ul class="navbar-nav ml-lg-auto">
                    <div class="ml-lg-4">
                                 
                        <i class="fa fa-user "> </i>
                        @User.Identity.Name
                         
                        
                    </div>
                </ul>

            
                            
                        </div>
                    </div>
                </ul>
            </div>
        </div>
    </nav>


    <hr />
    @RenderBody()

    @await RenderSectionAsync("Scripts", required: false)
