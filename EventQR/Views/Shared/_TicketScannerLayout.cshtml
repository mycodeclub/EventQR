    <script src="/uitemplate/scannerui/js/Headroom.js"></script>
    <script src="/uitemplate/scannerui/js/jQuery.headroom.js"></script>
    <script src="/uitemplate/scannerui/js/owl.carousel.min.js"></script>
    <script src="/uitemplate/scannerui/js/smoothscroll.js"></script>
    <script src="/uitemplate/scannerui/js/custom.js"></script>

